{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install --force",
  "build": {
    "env": {
      "DISABLE_ESLINT_PLUGIN": "true",
      "VERCEL_FORCE_NO_BUILD_CACHE": "1"
    }
  }
}